<!-- messages.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Messages</title>
</head>
<body>
    <h1>Messages</h1>
    <ul>
        <% messages.forEach(function(message) { %>
            <li><%= message.text %> - <%= message.user %> - <%= message.added %></li>
        <% }); %>
    </ul>
</body>
</html>
